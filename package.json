{
    "name": "browsertools",
    "version": "0.1.73",
    "author": "dcruvolo",
    "license": "MIT",
    "description": "This package contains a series of functions for running JavaScript functions in shiny.",
    "keywords": [
        "r",
        "shiny",
        "js"
    ],
    "scripts": {
        "clean": "rm -r inst/browsertools/browsertools.min.*",
        "start": "parcel serve inst/browsertools/src/index.js --target assets",
        "build": "parcel build inst/browsertools/src/index.js --target assets",
        "demo_start": "parcel serve inst/browsertools-demo/src/index.js --target demo",
        "demo_build": "parcel build inst/browsertools-demo/src/index.js --target demo",
        "demo_clean": "rm -rf inst/browsertools-demo/www/demo.min.*"
    },
    "devDependencies": {
        "autoprefixer": "9.8.6",
        "parcel": "^2.0.0-beta.1"
    },
    "assets": "inst/browsertools/browsertools.min.js",
    "demo": "inst/browsertools-demo/www/demo.min.js",
    "targets": {
        "main": false,
        "assets": {
            "distDir": "inst/browsertools/",
            "sourceMap": false
        },
        "demo": {
            "distDir": "inst/browsertools-demo/www",
            "sourceMap": false
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/davidruvolo51/browsertools.git"
    },
    "bugs": {
        "url": "https://github.com/davidruvolo51/browsertools/issues"
    }
}
